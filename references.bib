@inproceedings{Bliss1997,
abstract = {Due to the frequency with which police and fire rescue personnel enter$\backslash$nunfamiliar buildings, and the complex and critical nature of such$\backslash$nspatial navigation tasks, the importance of finding new and improved$\backslash$nways to train route navigation is becoming paramount. The aim of$\backslash$nthe current project is to compare four methods for training police$\backslash$nofficers to acquire and display knowledge about spatial navigation$\backslash$nin an unfamiliar building. Sixty police officers from the Huntsville,$\backslash$nAlabama area will be trained to navigate through the Administrative$\backslash$nScience building at The University of Alabama in Huntsville. The$\backslash$nofficers, who will not have had experience with the Administrative$\backslash$nScience building prior to the experiment: will be randomly assigned$\backslash$nto receive one of four methods for building navigation training:$\backslash$nVerbal Directions, Blueprints, Virtual Reality Training, or Actual$\backslash$nBuilding Training (Control). After training, speed and accuracy of$\backslash$nnavigation in the actual building will be measured. Participants$\backslash$nwill be required to use the shortest route to locate a mock baby$\backslash$n(a life-sized doll) in a fixed location. Once the doll is located,$\backslash$nofficers will be required to leave the building using the nearest$\backslash$nexit. Time taken to locate the doll, time taken to reach the exit,$\backslash$nand errors (wrong turns) made in doing so, will be compared among$\backslash$ngroups by using analyses of variance (ANOVAs). Results will be discussed$\backslash$nwith regard to theories concerning task fidelity and transfer of$\backslash$ntraining, and human performance in virtual environments.},
author = {Bliss, James P and Tidwell, Philip D},
booktitle = {Proceedings of the Human Factors and Ergonomics Society Annual Meeting},
doi = {10.1177/154193129503901412},
file = {:Users/andrew/Documents/Mendeley Desktop/Bliss, Tidwell/Bliss, Tidwell - 1997 - The Effectiveness of Virtual Reality for Administering Spatial Navigation Training to Firefighters.pdf:pdf},
isbn = {1054-7460},
issn = {1071-1813},
pages = {73--86},
title = {{The Effectiveness of Virtual Reality for Administering Spatial Navigation Training to Firefighters}},
volume = {6},
year = {1997}
}
@article{Brown1989,
abstract = {In most work investigating factors influencing the success of analogies in instruction, an underlying assumption is that students have little or no knowledge of the target situation (the situation to be explained by analogy). It is interesting to ask what influences the success of analogies when students believe they understand the target situation. If this understanding is not normative, instruction must aim at conceptual change rather than simply conceptual growth. Through the analysis of four case studies of tutoring interviews (two of which achieved some noticeable conceptual change and two of which did not) we propose a preliminary list of factors important for success in overcoming misconceptions via analogical reasoning. First, there must be a usable anchoring conception. Second, the analogical connection between an anchoring example and the target situation may need to be developed explicitly through processes such as the use of intermediate, ldquobridgingrdquo analogies. Third, it may be necessary to engage the student in a process of analogical reasoning in an interactive teaching environment, rather than simply presenting the analogy in tetext or lecture. Finally, the result of this process may need to be more than analogical transfer of abstract relational structure. The analogies may need to be used to enrich the target situation, leading to the student's construction of a new explanatory model.},
author = {Brown, David E. and Clement, John},
doi = {10.1007/BF00118013},
file = {:Users/andrew/Documents/Mendeley Desktop/Brown, Clement/Brown, Clement - 1989 - Overcoming misconceptions via analogical reasoning abstract transfer versus explanatory model construction.pdf:pdf},
issn = {00204277},
journal = {Instructional Science},
number = {4},
pages = {237--261},
title = {{Overcoming misconceptions via analogical reasoning: abstract transfer versus explanatory model construction}},
volume = {18},
year = {1989}
}
