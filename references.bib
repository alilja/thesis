@phdthesis{Gadgil2012,
abstract = {Prior research on conceptual change has identified multiple kinds of misconceptions at different levels of representational complexity including false beliefs, flawed mental models, and incorrect ontological categories. We hypothesized that conceptual change of a mental model requires change in the system of relations between the features of the prior model. To test this hypothesis, we compared instruction aimed at revising knowledge at the mental model level called holistic confrontation - in which the learner compares and contrasts a diagram of his or her flawed mental model to an expert model - to instruction aimed at revising knowledge at the false belief level - in which the learner is prompted to self-explain the expert model alone. We found evidence that participants who engaged in holistic confrontation were more likely to acquire a correct mental model, and a deeper understanding of the systems of relations in the model than those who were prompted to self-explain the expert model. The results are discussed in terms of their implications for science instruction. ?? 2011 Elsevier Ltd.},
author = {Gadgil, Soniya and Nokes-Malach, Timothy J. and Chi, Michelene T H},
booktitle = {Learning and Instruction},
doi = {10.1016/j.learninstruc.2011.06.002},
file = {:Users/andrew/Documents/Mendeley Desktop/Gadgil, Nokes-Malach, Chi/Gadgil, Nokes-Malach, Chi - 2012 - Effectiveness of holistic mental model confrontation in driving conceptual change.pdf:pdf},
isbn = {0959-4752},
issn = {09594752},
keywords = {Comparison,Conceptual change,Learning,Mental models,Self-explanation},
pages = {47--61},
school = {University of Pittsburgh},
title = {{Effectiveness of holistic mental model confrontation in driving conceptual change}},
volume = {22},
year = {2012}
}
